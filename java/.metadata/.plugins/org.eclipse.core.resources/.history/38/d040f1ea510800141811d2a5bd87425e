package accesories;

import java.io.*;
import java.util.*;

public class Scale {
	public static void main(String[] args) throws FileNotFoundException {
	    HashMap<String, String> map = new HashMap<String, String>();

	    BufferedReader reader = new BufferedReader(new FileReader("accesories.txt"));
	    String line = "";
	    while ((line = reader.readLine()) != null) {
	      String token[] = line.split(" ");
	      map.put(token[0], token[1]);
	    }
	    reader.close();

	    List<Map.Entry<String, String> list =
	        new ArrayList<Map.Entry<String, String>(map.entrySet());

	    Collections.sort(list, Comparator<Map.Entry<String, String>>() {
	      @Override
	      public int compare(Entry<String, String> a, Map.Entry<String, String> b) {
	        return a.value.compareToIgnoreCase(b.value);
	      }
	    });

	    System.out.println(accesories);
	    }
}
